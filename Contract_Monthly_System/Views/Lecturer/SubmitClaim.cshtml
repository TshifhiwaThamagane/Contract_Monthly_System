@model Contract_Monthly_System.Models.LectureViewer

<h2>Lecturer Dashboard</h2>

<p>Submit a new claim:</p>
<button>Submit Claim</button>
<button>Upload Supporting Documents</button>

<h3>My Claims</h3>
<table>
    <thead>
        <tr><th>Month</th><th>Module</th><th>Hours</th><th>Total</th><th>Status</th></tr>
    </thead>
    <tbody>
        @foreach (var claim in Model.Claims)
        {
            <tr>
                <td>@claim.Month</td>
                <td>@claim.Module</td>
                <td>@claim.Hours</td>
                <td>@claim.Total</td>
                <td>@claim.State</td>
            </tr>
        }
    </tbody>
</table>
